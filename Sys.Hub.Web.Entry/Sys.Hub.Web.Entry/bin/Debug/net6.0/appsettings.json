{
  "$schema": "https://gitee.com/dotnetchina/Furion/raw/v4/schemas/v4/furion-schema.json",
  //配置日志文件
  "Logging": {
    "File": {
      "Append": true,
      "MinimumLevel": "Information",
      "FileSizeLimitBytes": 102400, //日志大于5M自动创建新的日志文件
      "MaxRollingFiles": 100
    }
  },

  "AllowedHosts": "*"
}
